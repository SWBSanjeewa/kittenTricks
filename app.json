{
	"expo": {
		"name": "Kitten Tricks",
		"slug": "kitten-tricks",
		"version": "2.4.0",
		"description": "UI Kitten Demo Application",
		"ios": {
			"bundleIdentifier": "com.cr4zyc4t.kittentricks",
			"buildNumber": "3"
		},
		"android": {
			"package": "com.cr4zyc4t.kittentricks",
			"versionCode": 3
		},
		"jsEngine": "hermes",
		"privacy": "public",
		"platforms": ["ios", "android", "web"],
		"orientation": "portrait",
		"userInterfaceStyle": "automatic",
		"icon": "./src/assets/images/image-app-icon.png",
		"splash": {
			"image": "src/assets/images/image-splash.png",
			"backgroundColor": "#F7F9FC",
			"resizeMode": "contain"
		},
		"androidStatusBar": {
			"barStyle": "dark-content",
			"backgroundColor": "#F7F9FC"
		},
		"assetBundlePatterns": ["src/assets/images/*", "src/assets/fonts", "src/layouts/**/assets/*"],
		"updates": {
			"fallbackToCacheTimeout": 20
		}
	}
}
